<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="DBParaConstructor" class="com.DBParaConstructor" >
        <constructor-arg name="sqlServerDriverClassName" value="com.DBParaConstructor"/>
        <constructor-arg name="sqlServerUrl" value="jdbc:mysql://localhost:3306/Test" />
        <constructor-arg name="sqlServerUserName" value="123" />
        <constructor-arg name="sqlServerPassword" value="123"/>
    </bean>

</beans>