<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="DBParaProperty" class="com.DBParaProperty">
        <property name="sqlServerDriverClassName" value="com.mysql.jdbc.Driver"/>
        <property name="sqlServerUrl" value="jdbc:mysql://localhost:3306/Test"/>
        <property name="sqlServerPassword" value="123" />
        <property name="sqlServerUserName" value="root"/>
    </bean>

</beans>