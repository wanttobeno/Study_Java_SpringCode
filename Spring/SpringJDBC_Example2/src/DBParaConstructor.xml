<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="DBParaConstructor" class="com.tutorialspoint.DBParaConstructor">
        <constructor-arg name="sqlServerDriverClassName" value="com.mysql.jdbc.Driver"></constructor-arg>
        <constructor-arg name="sqlServerUrl" value="jdbc:mysql://localhost:3306/TEST"></constructor-arg>
        <constructor-arg name="sqlServerUserName" value="root"></constructor-arg>
        <constructor-arg name="sqlServerPassword" value="123"></constructor-arg>
    </bean>
</beans>